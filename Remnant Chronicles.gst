          <rules>
            <rule id="d4ce-9fe5-ae9e-83f4" name="Recover Casualties" hidden="false">
              <description>X victory points for every one of your casualties brought to within 5” of your home edge.
X equals the rank of the casualty.

</description>
            </rule>
          </rules>
          <categoryLinks>
            <categoryLink id="5622-e223-8eae-118b" name="Tatical Cards" hidden="false" targetId="0658-a548-6451-1495" primary="false"/>
