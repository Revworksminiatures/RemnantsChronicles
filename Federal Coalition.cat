                        </selectionEntry>
                        <selectionEntry id="d4b4-3872-1fd5-6521" name="Targeting Laser" hidden="false" collective="false" import="true" type="upgrade">
                          <rules>
                            <rule id="452b-36b8-00e3-9b1a" name="Targeting Laser" hidden="false">
                              <description>When a model acts as a forward observer for a model in another unit the targeting laser adds a single advantage to that attack roll.</description>
                            </rule>
                          </rules>
